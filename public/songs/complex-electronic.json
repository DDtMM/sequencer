{
  "$schema": "./song.schema.json",
  "title": "Complex Electronic",
  "bpm": 128,
  "beatsPerBar": 4,
  "masterChannel": {
    "id": "master",
    "effects": [
      {
        "name": "reverb",
        "decay": 3.0
      }
    ]
  },
  "channels": [
    {
      "id": "kick",
      "effects": [
        {
          "name": "distortion",
          "distortion": 0.3
        }
      ]
    },
    {
      "id": "melody",
      "pan": 0.2,
      "effects": [
        {
          "name": "delay",
          "delayTime": "16n",
          "feedback": 0.4
        },
        {
          "name": "filter",
          "frequency": 2000,
          "type": "highpass"
        }
      ]
    },
    {
      "id": "arp",
      "pan": -0.2,
      "effects": [
        {
          "name": "chorus",
          "frequency": 2.0,
          "delayTime": 2.5,
          "depth": 0.5
        }
      ]
    }
  ],
  "instruments": [
