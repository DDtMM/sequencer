          "envelope": {
            "a": 0.001,
            "d": 0.5,
            "s": 0,
            "r": 0
          },
          "effects": [
            {
              "name": "bitCrusher",
              "bits": 4,
              "wet": 0.5
            }
          ]
        }
      ]
    },
    {
      "id": "melody",
      "generators": [
        {
          "type": "oscillator",
          "oscillator": {
            "type": "triangle"
          },
          "envelope": {
            "a": 0.02,
            "d": 0.15,
            "s": 0.4,
            "r": 0.3
          },
          "effects": [
            {
              "name": "distortion",
              "distortion": 1,
              "wet": 1
            }
          ]
        }
      ]
    },
    {
      "id": "arp",
      "generators": [
        {
          "type": "oscillator",
          "oscillator": {
            "type": "square"
          },
          "envelope": {
            "a": 0.005,
            "d": 0.05,
            "s": 0.2,
            "r": 0.1
          }
        }
      ]
    }
